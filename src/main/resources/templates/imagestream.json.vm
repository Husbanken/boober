{
  "kind": "ImageStream",
  "apiVersion": "v1",
  "metadata": {
    "name": "${adc.name}",
    "labels": {
      "app": "${adc.name}",
      "updatedBy": "${username}",
      "affiliation": "${adc.affiliation}"
    }
  }
    #if (${adc.type} == "deploy")
    ,
      "spec": {
        "dockerImageRepository": "${dockerRegistry}/${dd.dockerGroup}/${dd.dockerName}",
        "tags": [
          {
              "name": "${dd.tag}",
              "from": {
                "kind": "DockerImage",
                "name": "${dockerRegistry}/${dd.dockerGroup}/${dd.dockerName}:${dd.version}"
              },
              "importPolicy": {
                "scheduled": true
              }
          }
        ]
      }
    #end
}
